{
  "name": "igniphp/container",
  "description": "PSR-11 compatible dependency container",
  "keywords": [
    "PSR-11",
    "Autowiring",
    "Fast",
    "Dependency Container",
    "Service Locator",
    "PHP 7"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dawid <krac> Kraczkowski",
      "email": "dawid.kraczkowski@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8",
    "psr/container": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Igni\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IgniTest\\": "tests/"
    }
  }
}
